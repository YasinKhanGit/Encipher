/*task 6*/
var a=7;
var b=3;
console.log(a+b);
console.log(a-b);
console.log(a*b);
console.log(a**b);
console.log(a%b);
console.log(a==b);
console.log(a===b);
console.log(a!=b);
console.log(a<b);
console.log(a>b);


