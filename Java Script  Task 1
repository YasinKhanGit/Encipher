/*task 10 */
var num1="5";
var num2="2";
var add_num=(num1+num2);
var sub_num=(num1-num2);
var mul_num=(num1*num2);
console.log(add_num)
console.log(sub_num)
console.log(mul_num)
console.log(typeof add_num)
console.log(typeof sub_num)
console.log(typeof mul_num)
